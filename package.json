{
  "name": "@lorcan-store/auto-icon",
  "version": "1.3.18",
  "description": "自动化从iconfont平台获取图标资源的工具",
  "main": "dist/index.js",
  "bin": {
    "auto-icon": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish:patch": "npm run version:patch && npm publish",
    "publish:minor": "npm run version:minor && npm publish",
    "publish:major": "npm run version:major && npm publish",
    "release": "npm run build && npm run publish:patch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:local": "jest src/__tests__/local.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lorcan-cloud/auto-icon.git"
  },
  "keywords": [
    "iconfont",
    "icon",
    "auto",
    "font"
  ],
  "author": "lorcan-store",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "dotenv": "^16.4.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ]
